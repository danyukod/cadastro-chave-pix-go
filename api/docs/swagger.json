                "accountHolderName": {
                    "type": "string"
                },
                "accountNumber": {
                    "type": "integer"
                },
                "accountType": {
                    "type": "string"
                },
                "agencyNumber": {
                    "type": "integer"
                },
                "id": {
                    "type": "string"
                },
                "pixKeyType": {
                    "type": "string"
                },
                "response": {
                    "type": "string"
                }
            }
        }
